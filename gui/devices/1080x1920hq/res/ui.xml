			<object type="image">
				<image resource="divider_wide" />
				<placement x="%center_x%" y="%row_nav_y%" placement="4" />
			</object>
			
			<object type="button">
				<condition var1="tw_busy" var2="0" />
				<placement x="%home_button_x%" y="%row_nav_y%" />
				<font resource="body1" color="%text_button%" />
				<image resource="home" highlightresource="home_hl" />
				<action function="key">home</action>
			</object>

			<object type="button">
				<condition var1="tw_busy" var2="0" />
				<condition var1="navbar_sams" var2="0" />
				<placement x="%back_button_x%" y="%row_nav_y%" />
				<font resource="body1" color="%text_button%" />
				<image resource="back" highlightresource="back_hl" />
				<action function="key">back</action>
			</object>

			<object type="button">
				<condition var1="tw_busy" var2="0" />
				<condition var1="navbar_sams" var2="1" />
				<placement x="%slideout_button_x%" y="%row_nav_y%" />
